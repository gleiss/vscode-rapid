{
	"name": "rapid",
	"publisher": "gleiss",
	"displayName": "Rapid",
	"description": "",
	"license": "MIT",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other",
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "rapid",
				"extensions": [
					".spec"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "rapid",
				"scopeName": "source.rapid",
				"path": "./syntaxes/rapid.tmGrammar.json"
			}
		],
		"snippets": [
			{
				"language": "rapid",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"vsce": "^1.59.0"
	}
}
